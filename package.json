{
  "name": "quackage",
  "version": "1.0.17",
  "description": "Building.  Testing.  Quacking.",
  "main": "source/Quackage.js",
  "scripts": {
    "start": "node source/Quackage.js",
    "coverage": "./node_modules/.bin/nyc --reporter=lcov --reporter=text-lcov ./node_modules/mocha/bin/_mocha -- -u tdd -R spec",
    "test": "./node_modules/.bin/mocha -u tdd -R spec",
    "buildtemplates": "node ./source/Quackage.js buildtemplates ./templates",
    "build": "./node_modules/.bin/gulp build"
  },
  "bin": {
    "quack": "./cli/quack",
    "qua": "./cli/quack"
  },
  "mocha": {
    "diff": true,
    "extension": [
      "js"
    ],
    "package": "./package.json",
    "reporter": "spec",
    "slow": "75",
    "timeout": "5000",
    "ui": "tdd",
    "watch-files": [
      "source/**/*.js",
      "test/**/*.js"
    ],
    "watch-ignore": [
      "lib/vendor"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenvelozo/quackage.git"
  },
  "browser": {
    "./source/utility/Quackage-Execute-Process.js": "./source/utility/Quackage-Execute-Process-Base.js"
  },
  "keywords": [
    "templates",
    "metapackage"
  ],
  "author": "Steven Velozo <steven@velozo.com> (http://velozo.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevenvelozo/quackage/issues"
  },
  "homepage": "https://github.com/stevenvelozo/quackage",
  "dependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "browserify": "^17.0.0",
    "chai": "4.3.7",
    "commander": "^11.0.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-env": "^0.4.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.1.0",
    "mocha": "10.2.0",
    "npm-check-updates": "^16.10.13",
    "nyc": "^15.1.0",
    "pict": "^1.0.113",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
